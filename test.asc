SET blue: $99c1f1

GRAPHREP layer:-1

AVAL robo: "rangeRobo"
AVAL min: "rangeMin"
AVAL max: "rangeMax"

AVAL active: "active"

FILL color:$9a9996
ELLIPSE rx:(CM max) ry:(CM max)
FILL color:$ffffff
ELLIPSE rx:(CM min) ry:(CM min)

IF (active = "1") {
  FILL color:$a51d2d
  ELLIPSE rx:(CM robo) ry:(CM robo)
}
ELSE {
  FILL color:$ffbe6f
  ELLIPSE rx:(CM robo) ry:(CM robo)
}

